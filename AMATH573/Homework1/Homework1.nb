Cell[CellGroupData[{

Cell["Part b", "Subsection",
 CellChangeTimes->{{3.873898728258328*^9, 
  3.873898729314848*^9}},ExpressionUUID->"c85e3cf0-8d8f-4391-a88e-\
c3383b9fb24e"],

Cell["\<\
Now, we experiment with solution to Burgers equation from part b to try to \
find a pattern.\
\>", "Text",
 CellChangeTimes->{{3.873898751271315*^9, 3.873898765317768*^9}, {
  3.873898809657669*^9, 
  3.873898826043788*^9}},ExpressionUUID->"63855e5d-eec1-4271-b509-\
b80f63325928"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"\[Theta]2", "[", 
    RowBox[{"x_", ",", "t_"}], "]"}], "=", 
   RowBox[{"1", "+", 
    RowBox[{"\[Alpha]2", "*", 
     RowBox[{"Exp", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"-", "K1"}], "*", "x"}], "+", 
       RowBox[{"\[Nu]2", "*", 
        RowBox[{"K1", "^", "2"}], "*", "t"}]}], "]"}]}], "+", 
    RowBox[{"\[Beta]2", "*", 
     RowBox[{"Exp", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"-", "K2"}], "*", "x"}], "+", 
       RowBox[{"\[Nu]2", "*", 
        RowBox[{"K2", "^", "2"}], "*", "t"}]}], "]"}]}]}]}], ";"}]], "Input",
 CellChangeTimes->{{3.8738987467214727`*^9, 3.873898748887899*^9}, {
   3.873898829740404*^9, 3.873898856667316*^9}, {3.873898993166072*^9, 
   3.873899023823748*^9}, 3.873899079712772*^9},
 CellLabel->"In[9]:=",ExpressionUUID->"125103a5-e619-4ab2-ba52-610b0345773d"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"u2", "[", 
    RowBox[{"x_", ",", "t_"}], "]"}], "=", 
   RowBox[{
    RowBox[{"-", "2"}], "*", "\[Nu]2", "*", 
    RowBox[{
     RowBox[{"D", "[", 
      RowBox[{
       RowBox[{"\[Theta]2", "[", 
        RowBox[{"x", ",", "t"}], "]"}], ",", 
       RowBox[{"{", "x", "}"}]}], "]"}], "/", 
     RowBox[{"\[Theta]2", "[", 
      RowBox[{"x", ",", "t"}], "]"}]}]}]}], ";"}]], "Input",
 CellChangeTimes->{{3.8738990451628237`*^9, 3.8738990815633373`*^9}, 
   3.873901884287867*^9},
 CellLabel->"In[10]:=",ExpressionUUID->"ae863ff6-7fe1-4146-bfac-f36cf57dfebd"],

Cell[TextData[{
 "Based on the behavior we observed in part a, we expect that \[Alpha] and \
\[Beta] will only shift our solution. Since \[Nu] is present in both terms, \
we should be able find the pattern by changing the ratio ",
 Cell[BoxData[
  RowBox[{
   SubscriptBox["k", "1"], "/", 
   SubscriptBox["k", "2"]}]],ExpressionUUID->
  "2c18d533-b315-4cff-bb01-fed7a5c7169e"],
 " as in problem 2. "
}], "Text",
 CellChangeTimes->{{3.873900419298764*^9, 3.873900462176223*^9}, {
  3.873900781195685*^9, 3.873900796213901*^9}, {3.8739010034742317`*^9, 
