 RowBox[{
  RowBox[{"\[Nu]2", " ", "=", " ", "Pi"}], ";", " ", 
  RowBox[{"K2", " ", "=", " ", "1"}], ";", " ", 
  RowBox[{"\[Alpha]2", " ", "=", " ", 
   RowBox[{"Exp", "[", "1", "]"}]}], ";", " ", 
  RowBox[{"\[Beta]2", " ", "=", " ", 
   RowBox[{"Exp", "[", 
    RowBox[{"-", "1"}], "]"}]}], ";", " ", 
  RowBox[{"K2", "=", "1"}], ";"}]], "Input",
